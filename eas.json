{
  "cli": {
    "version": ">= 9.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m1-medium",
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": true,
      "releaseChannel": "staging",
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "image": "macos-sequoia-15.3-xcode-16.2"
      },
      "android": {
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "apps@digiboost.com",
        "ascAppId": "6502469400",
        "appleTeamId": "ZF76595MC3"
      }
    }
  }
}
